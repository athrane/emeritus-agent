{
    "name": "emeritus-agent",
    "version": "1.0.0",
    "type": "module",
    "main": "src/main.js",
    "scripts": {
        "start": "node src/main.js",
        "start:ecs": "node src/main2.js",
        "build": "esbuild src/main.js --bundle --outfile=dist/bundle.js --format=esm --target=esnext",
        "build:ecs": "esbuild src/main2.js --bundle --outfile=dist/bundle.ecs.js --format=esm --target=esnext",
        "test": "jest --config jest.config.js --coverage"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@babel/preset-env": "^7.26.9",
        "babel-jest": "^29.7.0",
        "esbuild": "^0.25.2",
        "jest": "^29.7.0"
    }
}
